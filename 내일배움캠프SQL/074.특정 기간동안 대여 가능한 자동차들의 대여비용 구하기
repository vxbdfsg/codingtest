SELECT C.CAR_ID,
C.CAR_TYPE,
ROUND(C.DAILY_FEE*30*(100-D.DISCOUNT_RATE)/100) AS FEE
FROM CAR_RENTAL_COMPANY_CAR C JOIN CAR_RENTAL_COMPANY_RENTAL_HISTORY R on C.CAR_ID = R.CAR_ID
        JOIN CAR_RENTAL_COMPANY_DISCOUNT_PLAN D on C.CAR_TYPE = D.CAR_TYPE
WHERE C.CAR_ID NOT IN ( 
        SELECT CAR_ID
        FROM CAR_RENTAL_COMPANY_RENTAL_HISTORY
        WHERE END_DATE >= '2022-11-01' AND START_DATE <= '2022-12-01'
        
) and D.DURATION_TYPE like '30%'
GROUP BY C.CAR_ID
HAVING C.CAR_TYPE IN ('세단', 'SUV') AND (FEE >= 500000 AND FEE < 2000000)
ORDER BY FEE DESC, C.CAR_TYPE, C.CAR_ID DESC
